<% @webSearch.each do |web| %>
  <div class="result-div" order="<%= countAndNext %>">
    <div class="content">
      <p>
        <span class="result-title"> <%= link_to web.title.html_safe, 'http://'+strip_tags(web.uri), target: "_blank" %></span><br />
        <%= web.content.html_safe %>
        <span class="result-uri"><%= link_to web.uri.html_safe, 'http://'+strip_tags(web.uri), target: "_blank"  %></span>
      </p>
    </div> 

    <div class="controls">
      <div class="rtab-container">
        <%= image_tag("thumbs_up.png", class: "check-image") %>
        <%= image_tag("x-mark.png", class: "delete-image") %>
      </div>
    </div>
  </div>
<% end %>